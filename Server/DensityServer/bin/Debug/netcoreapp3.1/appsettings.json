{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=PreFlightAI;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Email": {
    "MailType": "SMTP",
    "MailServer": "localhost",
    "MailPort": 25,
    "UseSSL": true,
    "UserId": "",
    "Password": "",
    "RemoteServerAPI": "",
    "RemoteServerKey": ""
  },
  "Employee": {
    "FirstName": "",
    "LastName": "",
    "Password": ""
    
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Server",
        "Args": {
          "configure": [
            {
              "Name": "Server",
              "Args": {
                "pathFormat": "Logs/log-{Date}.txt",
                "outputTemplate": "[Server] {Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
              }
            }
          ]
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
